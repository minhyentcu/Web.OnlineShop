@model List<Web.OnlineShop.Entity.ProductCategory>

<div class="categories">
    <ul>
        <h3>Danh mục sản phẩm</h3>
        @foreach (var item in Model)
        {
            <li><a href="/san-pham/@item.MetaTitle-@item.Id">@item.Name (@item.Products.Count())</a></li>
        }
    </ul>
</div>
